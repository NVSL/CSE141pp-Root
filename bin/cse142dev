#!/usr/bin/env bash

set -e

if [ "$CSE142L_ROOT." = "." ]; then
    MY_PATH="`dirname \"$0\"`"              # relative
    MY_PATH="`( cd \"$MY_PATH\" && pwd )`"
    CSE142L_ROOT=$MY_PATH/..
    echo $CSE142L_ROOT
fi

cd $CSE142L_ROOT
. .bootstrap-venv/bin/activate
. env.sh
env

cse142 dev --publish published=8888,target=8888 $@

